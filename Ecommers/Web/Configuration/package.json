{
    "name": "ecommers",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build:css": "tailwindcss -i ../../wwwroot/css/input.css -o ../../wwwroot/css/output.css --minify",
        "build:js": "node build.js",
        "build": "npm run build:css && npm run build:js",
        "watch:css": "tailwindcss -i ../../wwwroot/css/input.css -o ../../wwwroot/css/output.css --watch",
        "watch:js": "node watch.js",
        "watch": "concurrently \"npm run watch:css\" \"npm run watch:js\""
    },
    "devDependencies": {
        "autoprefixer": "^10.4.22",
        "esbuild": "^0.27.0",
        "eslint": "^9.39.1",
        "glob": "^13.0.0",
        "postcss": "^8.5.6",
        "tailwindcss": "^3.4.18"
    },
    "dependencies": {
        "datatables.net": "^2.3.5",
        "datatables.net-dt": "^2.3.5",
        "datatables.net-responsive-dt": "^3.0.7",
        "dayjs": "^1.11.19",
        "jquery": "^3.7.1",
        "notyf": "^3.10.0",
        "shepherd.js": "^14.5.1",
        "sweetalert2": "^11.26.12",
        "zod": "^4.1.13"
    }
}
